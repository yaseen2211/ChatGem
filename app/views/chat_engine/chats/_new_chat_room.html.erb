<div class="modal fade" id="new-chat-room" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title" id="myCenterModalLabel">New Chat</h4>
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
      </div>
      <div class="modal-body">
        <div id="users-list" class="list-group" style="height: 200px;overflow: auto">
          <div class=" media-list">
            <ul class="vertical nodata_list">
              <%# User.where.not(id: ChatEngine::ChatSubscriber.remaining_subscriber_ids(current_user.id).uniq).each do |user| %>
              <%# ative_subscriber_ids = current_user.chats.get_active_subscriber_ids(current_user)%>
              <%# remaining_subscriber_ids = ChatEngine::ChatSubscriber.get_except_array(ative_subscriber_ids ) %>
              <%# User.where(id: remaining_subscriber_ids).each do |user| %>
              <% User.where.not(id: current_user.id).each do |user| %>
                <li class="li-style">
                  <a href="<%= chats_path({user_type: user.class.to_s,user_id: user.id }) %>" data-method="post" class="media border-0">
                    <div class="media-left pr-1">
                      <!--Avatar on Inbox Page-->
                        <span class="avatar avatar-md avatar-<%= user.online ? "online":"off" %>">
                          <%= image_tag "users/default-profile.png",  alt: "user-image",class: "media-object rounded-circle" %>
                          <i></i>
                        </span>
                    </div>
                    <div class="media-body w-100">
                        <h6 class="list-group-item-heading"><%= user.display_name %> </h6>
                    </div>
                  </a>
                </li>
              <% end %> 
            </ul>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" data-dismiss="modal"><%= t('close') %></button>
      </div>
    </div>
  </div>
</div>